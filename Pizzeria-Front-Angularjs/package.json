    "url": "https://github.com/isidrok/pizzashop-angular-front/issues"
  },
  "homepage": "https://github.com/isidrok/pizzashop-angular-front#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "json-server": "^0.10.0",
    "npm-run-all": "^4.0.2",
    "postcss-cssnext": "^2.11.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-livereload": "^0.4.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.4.1",
